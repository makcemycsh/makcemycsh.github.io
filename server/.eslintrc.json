{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "jquery": true
  },
  "rules": {
    "no-unused-vars": [
      0,
      {
        "vars": "all",
        "args": "after-used"
      }
    ],
    "key-spacing": [
      "error",
      {
        "align": "value"
      }
    ],
    "no-multi-spaces": "off",
    "no-console": "off",
    "valid-jsdoc": "off",
    "require-jsdoc": [
      "error",
      {
        "require": {
          "FunctionDeclaration": false,
          "MethodDefinition": false,
          "ClassDeclaration": false,
          "ArrowFunctionExpression": false,
          "FunctionExpression": false
        }
      }
    ],
    "no-invalid-this": "off",
    "max-len": [
      "error",
      {
        "code": 120,
        "ignoreComments": true
      }
    ],
    "no-undef": "off"
  },
  "extends": [
    "eslint:recommended",
    "google"
  ]
}
